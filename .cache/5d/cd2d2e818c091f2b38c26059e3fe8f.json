urce":"src/workout.js","original":{"line":57,"column":11}},{"generated":{"line":101,"column":46},"source":"src/workout.js","original":{"line":57,"column":4}},{"generated":{"line":102,"column":0},"source":"src/workout.js","original":{"line":60,"column":3}}],"sources":{"src/workout.js":"import Countdown from \"./countdown\";\nimport duration from \"duration-fns\";\n\nexport default class Workout {\n  constructor(repository) {\n    this.steps = [\n      {\n        type: \"warmup\",\n        order: 1,\n        duration: \"PT00M00S\",\n        description: \"Warmup\"\n      }\n    ];\n    this.name = \"\";\n    this.repository = repository;\n  }\n\n  start(onTick) {\n    const countdown = new Countdown(this.steps[0].duration, 1000);\n    countdown.start(onTick);\n  }\n\n  save() {\n    this.repository.save(this);\n  }\n\n  addStep(step) {\n    this.steps = [...this.steps, { ...step, order: this.getNextStep() }];\n  }\n\n  removeStep(step) {\n    this.steps = this.steps\n      .filter((s) => {\n        return s.order !== step.order;\n      })\n      .map((s, i) => {\n        s.order = i + 1;\n        return s;\n      });\n  }\n\n  updateStep(step) {\n    this.steps = this.steps.map((s) => {\n      if (step.order === s.order) {\n        return step;\n      }\n\n      return s;\n    });\n  }\n\n  getNextStep() {\n    return this.steps.length + 1;\n  }\n\n  getDuration() {\n    return this.steps.reduce((acc, step) => {\n      return duration.sum(acc, step.duration);\n    }, duration.parse(\"PT0M0S\"));\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"f0c3f3d62ea4b45ebda449a3ae08bc10","cacheData":{"env":{}}}